<form method="post" action="/all_animals">

  <label for="name">Name:</label>
  <input type="text" name="name" id="name" />

  <label for="age">Age:</label>
  <input type="text" name="age" id="age" />

  <label for="type">Type:</label>
  <input type="text" name="type" id="type" />

  <label for="breed">Breed:</label>
  <input type="text" name="breed" id="breed" />

  <label for="adopt_status">Adoption Status:</label>
  <select class="" name="adopt_status" id="adopt_status">
    <option value="true">True</option>
    <option value="false">False</option>
  </select>

  <label for="admittion_date">Admittion Status:</label>
  <input type="text" name="admittion_date" id="admittion_date" />

  <input type="submit" value="Create Animal"/>

</form>
